@page
@model LatinMedia.web.Pages.Admin.profileModel
@{
    ViewData["Title"] = "اطلاعات پروفایل";
}

<div class="col-md-12 col-sm-12 col-xs-12">
    <div class="x_panel">
        <div class="x_title">
            <h2>
                اطلاعات کاربری شما

            </h2>
            <ul class="nav navbar-right panel_toolbox">
                <li>
                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                </li>


            </ul>
            <div class="clearfix"></div>
        </div>
        <div class="x_content">
            <p> نام و نام خانوادگی : @(Model.informationUserViewModel.FirstName + " " + Model.informationUserViewModel.LastName)</p>
            <p> شماره موبایل : @Model.informationUserViewModel.Mobile</p>
            <p>ایمیل : @Model.informationUserViewModel.Email</p>
            <p><img class="img-circle" height="100" src="/UserAvatar/@Model.informationUserViewModel.UserAvatar" /></p>
        </div>
    </div>
</div>

